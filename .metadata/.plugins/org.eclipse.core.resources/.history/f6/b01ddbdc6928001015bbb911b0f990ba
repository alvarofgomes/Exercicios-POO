import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		Media m = new Media();
		
		System.out.println("Noma: ");
		m.setName(sc.nextLine());
		System.out.println("Digite A 1ยบ Nota Do Aluno: ");
		m.setN1(sc.nextDouble());
		System.out.println("Digite A 2ยบ Nota Do Aluno: ");
		m.setN2(sc.nextDouble());
		System.out.println("Digite A 3ยบ Nota Do Aluno: ");
		m.setN3(sc.nextDouble());
		
		System.out.println(m.Resto());
		
	}

}
