public class Produto {

	public String nome;
	public double preco;
	public int estoque;
	
	public double tot() {
		
		return preco * estoque;
		
	}
	
	public int AdicionarProdutos(int qtd) {
		
		int qt = estoque + qtd;
		
		return qt;
		
	}
	
	/*public int Removerprodutos() {
		
		
		
	}*/
	
}
